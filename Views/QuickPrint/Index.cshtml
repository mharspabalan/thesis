@using RCCW.Models;
@model QuickMarkModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Index</title>
</head>
<body>
    <div style="width:400px;">
       <div style="width:100%;height:200px;background-color:royalblue">
           <div style="width:400px;position:absolute;top:20px;height:50px;background-color:gold;padding-top:10px;">
                <img style="position:absolute;top:-10px;" width="84" height="84" src="../../Content/NCST_logo.png" />
                <h5 style="width:70%;position:absolute;top:-10px;left:80px;text-align:center">NATIONAL COLLEGE OF SCIENCE AND TECHNOLOGY</h5>
               <small style="width:70%;font-size:8px;position:absolute;top:40px;left:80px;text-align:center">Amafel Building, Aguinaldo Highway, Dasmariñas, Cavite 4114</small>
           </div>
            <div style="position:relative;top:92px;left:10px;width:40%">
                <img src="@Model.URL" />
            </div>
           <div style="float:right;padding-right:100px;width:180px">
               <p>
                   StudentID : @Model.StudentId<br />
                   Name      : @Model.Name<br />
                   Course    : @Model.Course<br />
                   Mac       : @Model.MacAddress<br />
               </p>
           </div>
       </div>
    </div>
</body>
</html>
